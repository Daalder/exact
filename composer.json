{
    "name": "daalder/exact",
    "description": "Exact integration for Daalder",
	"license": "MIT",
    "authors": [
        {
            "name": "Egbert Wietses",
            "email": "info@pionect.nl"
        },
        {
            "name": "Vincent van Hoven"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/pionect/"
        }
    ],
    "require": {
        "picqer/exact-php-client": "^3.31"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3",
        "pionect/daalder": "^11|^12"
    },
    "autoload": {
        "classmap": ["database"],
        "psr-4": {
            "Daalder\\Exact\\": "src/",
            "Daalder\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Daalder\\Exact\\ExactServiceProvider"
            ]
        }
    }
}
